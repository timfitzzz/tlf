{
  "name": "gatsby-starter-typescript-rebass-netlifycms",
  "description": "My standard gatsby setup",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache && yarn start",
    "develop": "concurrently npx netlify-cms-proxy-server & nodemon --exec \"gatsby develop\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "lint": "eslint . --ext ts --ext tsx",
    "prettier-write": "prettier --write \"src/**/*.js\"",
    "release": "auto shipit",
    "start": "yarn develop",
    "test": "jest --verbose",
    "type-check": "tsc --pretty --noEmit",
    "install-husky": "node ./node_modules/.bin/husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn prettier-write",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "prettier": {
    "bracketSpacing": true,
    "proseWrap": "always",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.120",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/rebass": "^4.0.7",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^1.1.0",
    "auto-release-cli": "2.3.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.12.1",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-graphql": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-plugin-typescript": "^2.0.3",
    "graphql-typescript-definitions": "^0.15.0",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-html-reporter": "^3.3.0",
    "lint-staged": "^8.1.0",
    "multer": "^1.4.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "react-test-renderer": "^17.0.1",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "typescript-styled-plugin": "^0.13.0",
    "webpack-notifier": "^1.7.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@emotion/styled": "^10.0.27",
    "@github-docs/frontmatter": "^1.3.1",
    "@mdx-js/mdx": "^0.16.6",
    "@mdx-js/tag": "^0.16.6",
    "@octokit/core": "^3.2.5",
    "@octokit/rest": "^18.0.15",
    "@octokit/types": "^6.8.2",
