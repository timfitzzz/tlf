{
  "name": "gatsby-starter-typescript-rebass-netlifycms",
  "description": "My standard gatsby setup",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache && yarn start",
    "develop": "concurrently npx netlify-cms-proxy-server & nodemon --exec \"gatsby develop\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "lint": "eslint . --ext ts --ext tsx",
    "prettier-write": "prettier --write \"src/**/*.js\"",
    "release": "auto shipit",
    "start": "yarn develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "type-check": "tsc --pretty --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn prettier-write",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "prettier": {
    "bracketSpacing": true,
    "proseWrap": "always",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.120",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/rebass": "^4.0.7",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^1.1.0",
    "auto-release-cli": "2.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-graphql": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-plugin-typescript": "^2.0.3",
    "graphql-typescript-definitions": "^0.15.0",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.0",
    "multer": "^1.4.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "typescript": "^3.2.4",
    "typescript-styled-plugin": "^0.13.0",
    "webpack-notifier": "^1.7.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/mdx": "^0.16.6",
    "@mdx-js/tag": "^0.16.6",
    "@reach/router": "^1.2.1",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-plugin-styled-components": "^1.10.0",
    "concurrently": "^5.3.0",
    "d3-ease": "^1.0.5",
    "docz-utils": "^0.13.6",
    "framer-motion": "^3.2.1",
    "gatsby": "^2.0.76",
    "gatsby-image": "^2.0.20",
    "gatsby-mdx": "^0.3.4",
    "gatsby-plugin-catch-links": "^2.0.9",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-netlify-cms": "^4.8.0",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-transition-link": "^1.20.5",
    "gatsby-source-filesystem": "^2.0.16",
    "gatsby-transformer-sharp": "^2.1.8",
    "gsap": "^3.6.0",
    "hast-util-to-string": "^1.0.1",
    "konva": "^7.2.2",
    "lodash": "^4.17.11",
    "marked": "^0.6.0",
    "netlify-cms-app": "^2.14.12",
    "netlify-cms-proxy-server": "^1.3.6",
    "netlify-cms-widget-markdown": "2.12.6",
    "netlify-cms-widget-mdx": "0.4.4",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1",
    "react-head": "^3.0.2",
    "react-konva": "^17.0.1-3",
    "react-pose": "^4.0.10",
    "react-powerplug": "^1.0.0",
    "rebass": "^4.0.7",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5",
    "unstated": "^2.1.1",
    "unstated-next": "^1.1.0",
    "use-image": "^1.0.7",
    "write": "^1.0.3"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/damassi/gatsby-starter-typescript-rebass-netlifycms"
  },
  "bugs": {
    "url": "https://github.com/damassi/gatsby-starter-typescript-rebass-netlifycms/issues"
  },
  "authors": [
    "Christopher Pappas <damassi.pappas@gmail.com"
